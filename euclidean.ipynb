    "import numpy as np\n",
    "\n",
    "\n",
    "num1 = int(input(\"enter a number\"))\n",
    "num2 = int(input(\"enter another\"))\n",
    "\n",
    "num3 = np.abs(num1-num2)\n",
    "\n",
    "\n",
    "k = 1\n",
    "while(num2 - k*num3 > 0):\n",
    "    k = k+1\n",
    "\n",
    "\n",
    "\n",
    "factor = k\n",
    "\n",
    "j = 1\n",
    "while(num3 - j*factor > 0):\n",
    "    j = j+1\n",
    "    \n",
    "\n",
    "ans = j\n",
    "print(ans)\n",
    "\n",
